doctype html
mixin input(step, id, content, value, type=text, autocomplete="") 
    label.inputLabel(id=step+id) #{content}
    input.defineInput(value=value , type=type , class = id + " form-control" placeholder=content aria-label=content aria-describedby=step+id autocomplete=autocomplete)


